{
  "hosting": {
    "site": "kelink",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
    // Dynamic Links will start with https://link.indonesiya.com/app
    "appAssociation": "AUTO",
    "rewrites": [ { "source": "/app/**", "dynamicLinks": true } ]
  }
}
